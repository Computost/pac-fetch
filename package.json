{
  "name": "pac-fetch",
  "version": "0.0.35",
  "description": "Simple console application to download the Power Platform CLI (pac)",
  "type": "module",
  "scripts": {
    "prebuild": "npm run set-version",
    "build": "tsc",
    "prestart": "npm run set-version",
    "start": "node --loader ts-node/esm src/cli.ts",
    "set-version": "genversion --es6 --double --semi src/version.ts"
  },
  "bin": {
    "pac-fetch": "dist/cli.js"
  },
  "files": [
    "README.md",
    "dist/"
  ],
  "keywords": [
    "pac",
    "Power Platform",
    "Microsoft"
  ],
  "main": "dist/index.js",
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^18.15.0",
    "@types/yauzl": "^2.10.0",
    "genversion": "^3.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "commander": "^10.0.0",
    "node-fetch": "^3.3.1",
    "yauzl": "^2.10.0"
  }
}
