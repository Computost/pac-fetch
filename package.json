{
  "name": "pac-fetch",
  "version": "0.0.30",
  "description": "Simple console application to download the Power Platform CLI (pac)",
  "type": "module",
  "scripts": {
    "prebuild": "npm run set-version",
    "build": "tsc",
    "prestart": "npm run set-version",
    "start": "node --loader ts-node/esm src/cli.ts",
    "set-version": "node --loader ts-node/esm setVersion.ts"
  },
  "bin": {
    "pac-fetch": "dist/cli.js"
  },
  "files": [
    "dist/"
  ],
  "keywords": [
    "pac",
    "Power Platform",
    "Microsoft"
  ],
  "main": "dist/index.js",
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/commander": "^2.12.2",
    "@types/node": "^16.9.6",
    "@types/yauzl": "^2.9.2",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "commander": "^8.2.0",
    "node-fetch": "^3.1.0",
    "validate-npm-package-name": "^3.0.0",
    "yauzl": "^2.10.0"
  }
}
