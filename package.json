{
  "name": "pac-fetch",
  "version": "0.0.35",
  "description": "Simple console application to download the Power Platform CLI (pac)",
  "type": "module",
  "scripts": {
    "prebuild": "npm run set-version",
    "build": "tsc",
    "prestart": "npm run set-version",
    "start": "ts-node src/cli.ts",
    "set-version": "genversion --es6 --double --semi src/version.ts",
    "test": "vitest"
  },
  "bin": {
    "pac-fetch": "dist/cli.js"
  },
  "files": [
    "README.md",
    "dist/"
  ],
  "keywords": [
    "pac",
    "Power Platform",
    "Microsoft"
  ],
  "main": "dist/index.js",
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/mock-fs": "^4.13.1",
    "@types/node": "^18.15.0",
    "@types/yauzl": "^2.10.0",
    "@types/yazl": "^2.4.2",
    "genversion": "^3.1.1",
    "mock-fs": "^5.2.0",
    "msw": "^1.2.1",
    "typescript": "^4.9.5",
    "vitest": "^0.29.8",
    "yazl": "^2.5.1"
  },
  "dependencies": {
    "commander": "^10.0.0",
    "cross-fetch": "^3.1.5",
    "yauzl": "^2.10.0"
  }
}
